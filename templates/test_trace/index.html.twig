{% extends 'base.html.twig' %}

{% block title %}Hello TestTraceController!{% endblock %}

{% block body %}
<h1>Liste des traces existantes</h1>

    <ul>
        {% for trace in traces %}
            <li>
                <a href="{{ path('app_test_show', {'typeTrace': constant('class', trace)}) }}">{{ trace.display
                    }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
